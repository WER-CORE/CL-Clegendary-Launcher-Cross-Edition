<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="135" d:DesignHeight="50"
             x:Class="CL.CustomItem.ItemManegerProfile"
             HorizontalAlignment="Center"
             Height="50">

	<Grid x:Name="MainGird"
          MinWidth="135"
          Height="50">

		<Border x:Name="FonBorder"
                BorderThickness="1"
                Background="#FF202020"
                CornerRadius="9"
                Height="50"/>

		<Image x:Name="IconAccountType"
               HorizontalAlignment="Left"
               Height="42"
               Margin="5,5,0,0"
               VerticalAlignment="Top"
               Width="45"
               Source="avares://CL/Assets/big-steve-face-2002298922 2.png"
               Stretch="Fill"
               RenderOptions.BitmapInterpolationMode="HighQuality"/>

		<TextBlock x:Name="NameAccount"
                   HorizontalAlignment="Left"
                   Margin="60,5,0,0"
                   TextWrapping="Wrap"
                   Text="TextBlock"
                   VerticalAlignment="Top"
                   Foreground="White"
                   Width="126"
                   Height="38"
                   FontFamily="Inter 18pt 18pt"
                   FontSize="11"/>

		<Grid x:Name="ClickSelectAccount"
              HorizontalAlignment="Stretch"
              Height="50"
              VerticalAlignment="Center"
              Width="135"
              Cursor="Cross"
              Background="#00000000"/>

		<Image x:Name="DeleteAccount"
               HorizontalAlignment="Left"
               Height="22"
               Margin="110,26,0,0"
               VerticalAlignment="Top"
               Width="22"
               Source="avares://CL/Assets/delete_1000dp_E8EAED_FILL0_wght400_GRAD0_opsz48.png"
               Stretch="UniformToFill"
               Cursor="Hand"
               RenderTransformOrigin="0.476,0.188"
               RenderOptions.BitmapInterpolationMode="HighQuality"/>
	</Grid>
</UserControl>
